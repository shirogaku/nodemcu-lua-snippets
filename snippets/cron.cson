'.source.lua':
  'CRON schedule':
    'prefix': 'cronsche'
    'body': 'cron.schedule("${1:* * * * *}", ${2:function(e)\r\n\t ${3:...execute callback...}\r\nend)})$0'
  'CRON reset':
    'prefix': 'cronres'
    'body': 'cron.reset()'
  'CRON handler':
    'prefix': 'cronhand'
    'body': 'handler(${1:function(e)\r\n\t ${2:...callback...}\r\nend)})$0'
  'CRON unschedule':
    'prefix': 'cronunsche'
    'body': 'unschedule()'
